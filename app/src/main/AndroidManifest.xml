<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yum.rudefarm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:hardwareAccelerated="false"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".LoginActivity"
            android:hardwareAccelerated="false"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".MainActivity"
            android:hardwareAccelerated="false"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".JoinActivity"
            android:hardwareAccelerated="false"
            android:label="@string/title_activity_join"></activity>
        <activity
            android:name=".BaseInfoActivity"
            android:hardwareAccelerated="false"></activity>
        <activity
            android:name=".FindPasswordActivity"
            android:hardwareAccelerated="false"></activity>
        <activity
            android:name=".CheckListAcitivity"
            android:hardwareAccelerated="false">
        </activity>
    </application>

</manifest>
